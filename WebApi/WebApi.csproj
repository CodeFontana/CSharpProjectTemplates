    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>376a5f80-d130-473b-bb32-49c2855690a4</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
